    "@vitest/eslint-plugin": "^1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.83.0",
    "tailwindcss": "^4.1.2",
    "typescript": "~5.6.3",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.1",
